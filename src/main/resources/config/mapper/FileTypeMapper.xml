<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiuGEN.mapper.FileTypeMapper">
    <!--通过类型id查找所有该类型下的后缀-->
    <select id="selFileTypeByTid" parameterType="int" resultType="fileType">
        select * from filetype where tid=#{parameter1}
    </select>


</mapper>